{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": false,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "spykertyminstaller",
  "mainBinaryName": "spykertyminstaller",
  "identifier": "com.spykertym.installer",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Spykertym Installer",
        "width": 1400,
        "height": 900,
        "minWidth": 800,
        "minHeight": 500,
        "decorations": false,
        "useHttpsScheme": true
      }
    ]
  }
}